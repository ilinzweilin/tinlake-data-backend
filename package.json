{
  "name": "tinlake-data-backend",
  "version": "0.1.0",
  "description": "nodejs/typescript application that will query the Ethereum JSON RPC interface on a continuous basis to log time series data to a database and expose this data via a Rest",
  "main": "index.ts",
  "scripts": {
    "server": "export NODE_ENV=development && ts-node src/server.ts",
    "worker": "export NODE_ENV=development && ts-node src/worker.ts",
    "start:prod": "export NODE_ENV=production && (npm run worker & npm run server)",
    "start:dev": "export NODE_ENV=development && (npm run worker &  npm run server)",
    "create_test_events": "ts-node src/create_test_events.ts",
    "lint": "node_modules/.bin/tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/centrifuge/tinlake-data-backend.git"
  },
  "keywords": [
    "npm"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/centrifuge/tinlake-data-backend/issues"
  },
  "homepage": "https://github.com/centrifuge/tinlake-data-backend#readme",
  "dependencies": {
    "apollo-server": "^2.6.9",
    "bn.js": "^5.0.0",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "date-fns": "^1.30.1",
    "dotenv": "^8.1.0",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "^0.1.1",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "nedb": "^1.8.0",
    "next": "^9.0.1",
    "node-cache": "^4.2.1",
    "tinlake": "https://github.com/centrifuge/tinlake.js.git"
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.5",
    "@types/ethjs-signer": "^0.1.0",
    "@types/graphql": "^14.2.2",
    "@types/nedb": "^1.8.8",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3"
  }
}
